{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
 
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off"
    }
}
